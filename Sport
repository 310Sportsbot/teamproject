package Main;

import java.util.ArrayList;

public class Sport {
	private String name;
	private ArrayList<String> training;
	
	public Sport() {
	}
	
	public Sport(String name) {
		this.name=name;
	}
	
	public void setName(String name) {
		this.name = name;
	}
	
	public String getName() {
		return this.name;
	}
}
